{
  "name": "test-01",
  "request": {
    "method": "POST",
    "query": {
      "term": "test",
      "filter": ["contains", "all"],
      "page": ["allOf", "present", ["equalTo", 10]]
    },
    "body": [
      "allOf",
      ["jsonpath", "name", ["equalTo", "john doe"]],
      [
        "jsonpath",
        "job",
        [
          "equalto",
          {
            "value": "DEV",
            "ignoreCase": true
          }
        ]
      ]
    ]
  },
  "response": {
    "status": 201
  }
}
